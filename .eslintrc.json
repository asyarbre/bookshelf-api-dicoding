{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "linebreak-style": 0,
      "max-len": "off",
      "no-console": "off",
      "quotes": "off",
      "no-unused-vars": "off",
      "padded-blocks": "off",
      "no-undef": "off",
      "object-curly-newline": "off"
    }
}
